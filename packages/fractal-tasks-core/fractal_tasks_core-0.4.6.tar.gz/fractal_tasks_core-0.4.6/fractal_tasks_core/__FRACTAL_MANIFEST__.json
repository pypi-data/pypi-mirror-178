{
    "manifest_version": "1",
    "task_list":
[
    {
        "name": "Create OME-ZARR structure",
        "input_type": "image",
        "output_type": "zarr",
        "default_args": {
            "num_levels": 2,
            "coarsening_xy": 2,
            "metadata_table": "mrf_mlf"
        },
        "executable": "create_zarr_structure.py",
        "meta": {"executor": "cpu-low"}
    },
    {
        "name": "Yokogawa to Zarr",
        "input_type": "zarr",
        "output_type": "zarr",
        "executable": "yokogawa_to_zarr.py",
        "meta": {"executor": "cpu-low", "parallelization_level": "image"}
    },
    {
        "name": "Replicate Zarr structure",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {"project_to_2D": true, "suffix": "mip"},
        "executable": "replicate_zarr_structure.py",
        "meta": {"executor": "cpu-low"}
    },
    {
        "name": "Maximum Intensity Projection",
        "input_type": "zarr",
        "output_type": "zarr",
        "executable": "maximum_intensity_projection.py",
        "meta": {"executor": "cpu-low", "parallelization_level": "image"}
    },
    {
        "name": "Cellpose Segmentation",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {"labeling_channel": "A01_C01"},
        "executable": "cellpose_segmentation.py",
        "meta": {"executor": "gpu", "parallelization_level": "image"}
    },
    {
        "name": "Measurement",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {
            "labeling_channel": "A01_C01",
            "level": 0,
            "measurement_table_name": "nuclei"
        },
        "executable": "measurement.py",
        "meta": {"executor": "cpu-low", "parallelization_level": "image"}
    },
    {
        "name": "Illumination correction",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {"overwrite": false, "background": 100},
        "executable": "illumination_correction.py",
        "meta": {"executor": "cpu-low", "parallelization_level": "image"}
    },
    {
        "name": "Napari workflows wrapper",
        "input_type": "zarr",
        "output_type": "zarr",
        "default_args": {
            "ROI_table_name": "FOV_ROI_table",
            "level": 0,
            "relabeling": true,
            "expected_dimensions": 3
        },
        "executable": "napari_workflows_wrapper.py",
        "meta": {"executor": "cpu-low", "parallelization_level": "image"}
    },
    {
        "name": "Create OME-ZARR structure (multiplexing)",
        "input_type": "image",
        "output_type": "zarr",
        "default_args": {
            "num_levels": 2,
            "coarsening_xy": 2,
            "metadata_table": "mrf_mlf"
        },
        "executable": "create_zarr_structure_multiplex.py",
        "meta": {"executor": "cpu-low"}
    }
]
}
